{
  "name": "itools-lite",
  "sorftName": "iTools-lite",
  "version": "0.0.1",
  "description": "iTools精简版，包含基本的框架的一些基础的功能。",
  "main": "./dist/electron/main.js",
  "scripts": {
    "dev": "node ./electron/dev.js",
    "build": "node ./electron/build.js",
    "test-pack": "cross-env electron-builder",
    "test-run": "cross-env electron ./dist/electron/main.js",
    "pack": "node ./electron/build.js && cross-env electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BingKui/iTools-lite.git"
  },
  "author": "康兵奎",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BingKui/iTools-lite/issues"
  },
  "homepage": "https://github.com/BingKui/iTools-lite#readme",
  "build": {
    "productName": "iTools",
    "appId": "cn.uiseed.itools",
    "directories": {
      "output": "dist"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://**.**.**.**:3001/download/"
      }
    ],
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "color-convert": "^2.0.0",
    "copy-text-to-clipboard": "^2.0.0",
    "dayjs": "^1.8.14",
    "diff": "^4.0.1",
    "highlight.js": "^9.15.6",
    "imagemin": "^6.1.0",
    "iview": "^3.3.3",
    "jimp": "^0.6.4",
    "js-base64": "^2.5.1",
    "markdown-it": "^8.4.2",
    "md5": "^2.2.1",
    "nedb": "^1.8.0",
    "pinyin-no-jieba": "^2.8.3",
    "qrcode": "^1.3.3",
    "qrcode-reader": "^1.0.4",
    "v-contextmenu": "^2.8.0",
    "vue": "^2.5.22",
    "vue-json-pretty": "^1.6.0",
    "vue-router": "^3.0.2",
    "vue-scrolly": "^0.9.3",
    "vue-smooth-dnd": "^0.8.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "cfonts": "^2.4.3",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "cross-spawn": "^6.0.5",
    "css-loader": "^2.1.1",
    "del": "^4.1.1",
    "electron": "^5.0.1",
    "electron-builder": "^20.39.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "multispinner": "^0.2.1",
    "node-pre-gyp": "^0.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.12.1",
    "sass-resources-loader": "^2.0.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.30.0",
    "webpack-build-notifier": "^1.0.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-hot-middleware": "^2.24.4"
  }
}
